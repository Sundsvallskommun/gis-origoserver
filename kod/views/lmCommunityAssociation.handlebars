<div class="container-fluid">
  <div class="row">
    <div class="col-md-4">
      <div class="card">
        <div class="card-header">
          <h2>{{{features.[0].properties.samfallighetsforeningsattribut.foreningensForetagsnamn}}}</h2>
        </div>
      </div>
      <div class="card">
        <div class="card-header">
          <h3>Föreningsinformation</h3>
        </div>
      </div>
                {{#each features}}
            <table class="table table-light">
              <tbody>
                  <tr class="table-active">
                    <td>
                        {{#if properties.samfallighetsforeningsattribut.foreningensForetagsnamn}}
                          Föreningens företagsnamn: {{properties.samfallighetsforeningsattribut.foreningensForetagsnamn}}<br>
                        {{/if}}
                        {{#if properties.samfallighetsforeningsattribut.organisationsnummer}}
                          Organisationsnummer: {{properties.samfallighetsforeningsattribut.organisationsnummer}}<br>
                        {{/if}}
                        {{#if properties.samfallighetsforeningsattribut.foreningstyp}}
                          Föreningstyp: {{properties.samfallighetsforeningsattribut.foreningstyp}}<br>
                        {{/if}}
                        {{#if properties.samfallighetsforeningsattribut.status}}
                          Status: {{properties.samfallighetsforeningsattribut.status}}<br>
                        {{/if}}
                    </td>
                  </tr>
                  <tr>
                    <td>
                          Adress:<br>
                          {{#if properties.samfallighetsforeningsattribut.utdelningsadress1}}
                            {{properties.samfallighetsforeningsattribut.utdelningsadress1}}<br>
                          {{/if}}
                          {{#if properties.samfallighetsforeningsattribut.coAdress}}
                            c/o {{properties.samfallighetsforeningsattribut.coAdress}}<br>
                          {{/if}}
                          {{#if properties.samfallighetsforeningsattribut.postnummer}}
                            {{properties.samfallighetsforeningsattribut.postnummer}} 
                          {{/if}}
                          {{#if properties.samfallighetsforeningsattribut.postort}}
                            {{properties.samfallighetsforeningsattribut.postort}}<br>
                          {{/if}}
                    </td>
                  </tr>
                  <tr class="table-active">
                    <td>
                        {{#if properties.samfallighetsforeningsattribut.sate}}
                          Säte: {{properties.samfallighetsforeningsattribut.sate}}<br>
                        {{/if}}
                        {{#if properties.samfallighetsforeningsattribut.foreningsstamma}}
                          Föreningsstämma: {{properties.samfallighetsforeningsattribut.foreningsstamma}}<br>
                        {{/if}}
                        {{#if properties.samfallighetsforeningsattribut.statsbidragsnummer}}
                          Statsbidragsnummer: {{properties.samfallighetsforeningsattribut.statsbidragsnummer}}<br>
                        {{/if}}
                        {{#if properties.samfallighetsforeningsattribut.status}}
                          Status: {{properties.samfallighetsforeningsattribut.status}}<br>
                        {{/if}}
                        {{#if properties.samfallighetsforeningsattribut.firmatecknareTillaggsinformation}}
                          Firmatecknare tilläggsinformation: {{properties.samfallighetsforeningsattribut.firmatecknareTillaggsinformation}}<br>
                        {{/if}}
                        {{#if properties.samfallighetsforeningsattribut.registreringsdatum}}
                          Registreringsdatum: {{properties.samfallighetsforeningsattribut.registreringsdatum}}<br>
                        {{/if}}
                        Räkenskapsår: 
                        {{#if properties.samfallighetsforeningsattribut.rakenskapsarFran}}
                          från {{properties.samfallighetsforeningsattribut.rakenskapsarFran}}
                        {{/if}}
                        {{#if properties.samfallighetsforeningsattribut.rakenskapsarTill}}
                           till {{properties.samfallighetsforeningsattribut.rakenskapsarTill}}<br>
                        {{/if}}
                         {{#if properties.objektidentitet}}
                          Objektidentitet:  {{properties.objektidentitet}}<br>
                        {{/if}}
                        {{#if properties.samfallighetsforeningsattribut.senasteAndringSFR}}
                          Senaste ändring: {{dateFormat properties.samfallighetsforeningsattribut.senasteAndringSFR "YYYY-MM-DD hh:mm:ss"}}<br>
                        {{/if}}
                    </td>
                  </tr>
              </tbody>
            </table>
             <table class="table">
              <div class="card">
                <div class="card-header">
                  <h3>Styrelsemedlemmar</h3>
                </div>
              </div>
              <tbody>
                       {{#each properties.styrelsemedlem}}
                        <tr>
                          <td>
                          {{#if namn}}
                            Namn: {{namn}}<br>
                          {{/if}}
                          {{#if medlemstyp}}
                            Medlemstyp: {{medlemstyp}}<br>
                          {{/if}}
                          {{#if styrelsefunktion}}
                            Styrelsefunktion: <b>{{styrelsefunktion}}</b><br>
                          {{/if}}
                          {{#if utdelningsadress1}}
                            Adress: {{utdelningsadress1}}<br>
                          {{/if}}
                          {{#if postnummer}}
                            Postnummer: {{postnummer}}<br>
                          {{/if}}
                          {{#if postort}}
                            Postort: {{postort}}<br>
                          {{/if}}
                          {{#if firmatecknare}}
                            Firmatecknare: Ja<br>
                          {{/if}}
                          {{#if anmarkning}}
                            Anmärkning: <em>{{anmarkning}}</em><br>
                          {{/if}}
                          </td>
                        </tr>
                        {{/each}}
              </tbody>
            </table>
            <table class="table">
              <tbody>
              <div class="card">
                <div class="card-header">
                  <h3>Styrelseinformation</h3>
                </div>
              </div>
